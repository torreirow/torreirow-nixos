            "mode": "absolute",
            "steps": [{ "color": "red" }, { "color": "orange", "value": 1 }, { "color": "green", "value": 7 }]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Days to expiration" },
            "properties": [
              {
                "id": "custom.cellOptions",
                "value": { "mode": "gradient", "type": "color-background" }
              }
            ]
          }
        ]
      },
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 15 },
      "id": 21,
      "options": {
        "cellHeight": "sm",
        "showHeader": true,
        "sortBy": [{ "desc": false, "displayName": "Status" }]
      },
      "targets": [
        {
          "expr": "(probe_ssl_earliest_cert_expiry{job=\"blackbox-technative\"} - time()) / 86400",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "title": "SSL Certificate Expiry (Days Left)",
      "transformations": [
        { "id": "reduce", "options": { "reducers": ["lastNotNull"] } },
        { "id": "organize", "options": { "renameByName": { "Field": "URL", "Last *": "Days to expiration" } } },
        { "id": "sortBy", "options": { "sort": [{ "desc": false, "field": "Days to expiration" }] } }
      ],
      "type": "table"
    }
